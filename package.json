{
  "name": "sms-survey-engine",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "cross-env NODE_ENV=production node src/server.js",
    "start:dev": "nodemon src/server.js",
    "docker:build": "docker build -t sms-survey .",
    "docker:up": "docker-compose -f docker-compose.prod.yml up -d --build",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "lint": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "eslint": "^8.50.0",
    "prettier": "^3.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.4",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "cross-env": "^7.0.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@aws-sdk/s3-request-presigner": "^3.978.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "pg": "^8.17.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "twilio": "^5.12.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  }
}
